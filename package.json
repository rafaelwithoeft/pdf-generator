{
  "name": "pdf-generator",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src",
    "lint": "eslint . --ext .ts && tsc --noemit",
    "test": "cross-env NODE_ENV=test jest --runInBand --verbose",
    "build": "BABEL_ENV=build NODE_ENV=production babel src --extensions \".ts\" --out-dir dist --source-maps --copy-files --no-copy-ignored --delete-dir-on-start --no-comments",
    "prepare": "husky install",
    "test:ci": "cross-env NODE_ENV=test jest --runInBand --bail --coverage --ci --logHeapUsage",
    "test:coverage": "cross-env NODE_ENV=test jest --runInBand --coverage"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "dotenv": "^16.3.1",
    "json-schema-faker": "^0.5.5",
    "mustache": "^4.2.0",
    "puppeteer": "^21.10.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/register": "^7.22.15",
    "@types/jest": "^29.5.7",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
